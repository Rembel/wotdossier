                            </d3:ChartPlotter>
                        </Grid>
                        <Grid>
                            <d3:ChartPlotter Name="Chart2" LegendVisibility="Hidden" Height="200" Width="600" MainHorizontalAxisVisibility="Visible" MainVerticalAxisVisibility="Visible" Background="Transparent"
                                      Margin="10,10,0,10">
                            </d3:ChartPlotter>
                        </Grid>
                        <Grid>
                            <d3:ChartPlotter Name="Chart3" LegendVisibility="Hidden" Height="200" Width="600" MainHorizontalAxisVisibility="Visible" MainVerticalAxisVisibility="Visible" Background="Transparent"
                                      Margin="10,10,0,10">
                            </d3:ChartPlotter>
                        </Grid>
                    </StackPanel>
                </ScrollViewer>
                
            </TabItem>
