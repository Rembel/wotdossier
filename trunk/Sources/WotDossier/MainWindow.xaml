    <DockPanel>
        <DockPanel.Resources>
            <DataTemplate x:Key="MasterTemplate" >
                <Image Source="{Binding Path=MarkOfMastery, Mode=OneWay, Converter={x:Static Converters:MarkOfMasteryImageConverter.Default}}" />
            </DataTemplate>
            <DataTemplate x:Key="TankIconTemplate" >
                <Image Source="{Binding Path=Icon, Mode=OneWay, Converter={x:Static Converters:TankIconImageConverter.Default}}" />
            </DataTemplate>
        </DockPanel.Resources>
        <Menu DockPanel.Dock="Top" Height="23" VerticalAlignment="Top">
            <MenuItem Header="Load" Command="{Binding LoadCommand}"></MenuItem>
            <MenuItem Header="Settings" Click="MenuItemSettings_Click"></MenuItem>
        </Menu>
        <TabControl Height="Auto" Name="tabControl1" Width="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" DockPanel.Dock="Bottom">
