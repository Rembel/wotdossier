<UserControl x:Class="WotDossier.Tabs.CommonTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:Converters="clr-namespace:WotDossier.Converters"
             xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.Resources>
        <ImageBrush x:Key="bcolor" ImageSource="..\Resources\Images\back.jpg" />
    </UserControl.Resources>
    <Grid  Background="{StaticResource bcolor}">
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1" HorizontalAlignment="Center">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock Foreground="White" FontSize="32" FontFamily="Arial Narrow" Grid.Row="0" Grid.Column="0" Text="{Binding Name}" HorizontalAlignment="Left"></TextBlock>
            <TextBlock Foreground="#606061" FontSize="12" FontFamily="Arial" Grid.Row="0" Grid.Column="2" Text="Дата регистрации: " HorizontalAlignment="Right"></TextBlock>
            <TextBlock Foreground="#606061" FontSize="12" FontFamily="Arial" Grid.Row="0" Grid.Column="3" Text="{Binding Created}" HorizontalAlignment="Right"></TextBlock>
            <TextBlock Foreground="#606061" FontSize="12" FontFamily="Arial" Grid.Row="1" Grid.Column="0" Text="Данные по состоянию на " HorizontalAlignment="Left"></TextBlock>
            <TextBlock Foreground="#606061" FontSize="12" FontFamily="Arial" Grid.Row="1" Grid.Column="1" Text="{Binding Updated}" HorizontalAlignment="Left"></TextBlock>
        </Grid>
        <Grid Grid.Row="2" Width="Auto" HorizontalAlignment="Center">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
                <RowDefinition Height="25"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="250"></ColumnDefinition>
                <ColumnDefinition Width="150"></ColumnDefinition>
                <ColumnDefinition Width="150"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock Foreground="#FFA759" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Text="Overall Results" FontFamily="sans-serif" FontSize="17" FontWeight="Bold"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="1" Grid.Column="0" Text="Battles Participated: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="1" Grid.Column="1" Text="{Binding BattlesCount, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding BattlesCountDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="1" Grid.Column="2" Text="{Binding BattlesCountDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="2" Grid.Column="0" Text="Victories:"></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="2" Grid.Column="1" DataContext="{Binding}">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValuePercentFormater.Default}" >
                        <Binding Path="Wins" />
                        <Binding Path="WinsPercent" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>
            <TextBlock Foreground="{Binding WinsPercentDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="2" Grid.Column="2">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValueDeltaPercentFormater.Default}" >
                        <Binding Path="WinsDelta" />
                        <Binding Path="WinsPercentDelta" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="3" Grid.Column="0" Text="Defeats:"></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="3" Grid.Column="1">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValuePercentFormater.Default}" >
                        <Binding Path="Losses" />
                        <Binding Path="LossesPercent" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>
            <TextBlock Foreground="{Binding LossesPercentDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=true}" TextAlignment="Right" Grid.Row="3" Grid.Column="2">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValueDeltaPercentFormater.Default}" >
                        <Binding Path="LossesDelta" />
                        <Binding Path="LossesPercentDelta" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="4" Grid.Column="0" Text="Battles Survived:"></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="4" Grid.Column="1">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValuePercentFormater.Default}" >
                        <Binding Path="SurvivedBattles" />
                        <Binding Path="SurvivedBattlesPercent" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>
            <TextBlock Foreground="{Binding SurvivedBattlesPercentDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="4" Grid.Column="2">
                <TextBlock.Text>
                    <MultiBinding Converter="{x:Static Converters:ValueDeltaPercentFormater.Default}" >
                        <Binding Path="SurvivedBattlesDelta" />
                        <Binding Path="SurvivedBattlesPercentDelta" />
                    </MultiBinding>
                </TextBlock.Text>
            </TextBlock>

            <TextBlock Foreground="#FFA759" Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="2" Text="Experience" FontFamily="sans-serif" FontSize="17" FontWeight="Bold"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="6" Grid.Column="0" Text="Total Experience: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="6" Grid.Column="1" Text="{Binding Xp, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding XpDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="6" Grid.Column="2" Text="{Binding XpDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="7" Grid.Column="0" Text="Average Experience per Battle: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="7" Grid.Column="1" Text="{Binding BattleAvgXp, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding BattleAvgXpDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="7" Grid.Column="2" Text="{Binding BattleAvgXpDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="8" Grid.Column="0" Text="Maximum Experience per Battle: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="8" Grid.Column="1" Text="{Binding MaxXp, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding MaxXpDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="8" Grid.Column="2" Text="{Binding MaxXpDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>

            <TextBlock Foreground="#FFA759" Grid.Row="9" Grid.Column="0" Grid.ColumnSpan="2" Text="Battle Performance" FontFamily="sans-serif" FontSize="17" FontWeight="Bold"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="10" Grid.Column="0" Text="Destroyed: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="10" Grid.Column="1" Text="{Binding Frags, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding FragsDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="10" Grid.Column="2" Text="{Binding FragsDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="11" Grid.Column="0" Text="Detected: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="11" Grid.Column="1" Text="{Binding Spotted, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding SpottedDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="11" Grid.Column="2" Text="{Binding SpottedDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="12" Grid.Column="0" Text="Hit Ratio: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="12" Grid.Column="1" Text="{Binding HitsPercents, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding HitsPercentsDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="12" Grid.Column="2" Text="{Binding HitsPercentsDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="13" Grid.Column="0" Text="Damage Caused: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="13" Grid.Column="1" Text="{Binding DamageDealt, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding DamageDealtDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="13" Grid.Column="2" Text="{Binding DamageDealtDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="14" Grid.Column="0" Text="Capture Points: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="14" Grid.Column="1" Text="{Binding CapturePoints, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding CapturePointsDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="14" Grid.Column="2" Text="{Binding CapturePointsDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="15" Grid.Column="0" Text="Defense Points: "></TextBlock>
            <TextBlock Foreground="#BABFBA" TextAlignment="Right" Grid.Row="15" Grid.Column="1" Text="{Binding DroppedCapturePoints, StringFormat='### ### ### ###'}"></TextBlock>
            <TextBlock Foreground="{Binding DroppedCapturePointsDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" TextAlignment="Right" Grid.Row="15" Grid.Column="2" Text="{Binding DroppedCapturePointsDelta, StringFormat='+0.;-0.;0.'}"></TextBlock>
        </Grid>
        <Grid Grid.Row="3" Width="Auto" HorizontalAlignment="Center">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="Auto"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"></ColumnDefinition>
                <ColumnDefinition Width="300"></ColumnDefinition>
                <ColumnDefinition Width="100"></ColumnDefinition>
                <ColumnDefinition Width="50"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock Foreground="#BABFBA" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="3" Text="Rating"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="3" Text="Value"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="0" Grid.Column="3" Grid.ColumnSpan="3" Text="Place"></TextBlock>
            <!--GR-->
            <Image Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/gr.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="1" Grid.Column="1" Text="GR"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="2" Grid.Column="1" Text="Global Rating"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="1" Grid.Column="2" Text="{Binding Rating_IntegratedValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_IntegratedValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="2" Grid.Column="2" Text="{Binding Rating_IntegratedValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="1" Grid.Column="3" Text="{Binding Rating_IntegratedPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_IntegratedPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="2" Grid.Column="3" Text="{Binding Rating_IntegratedPlaceDelta}"></TextBlock>
            <!--W/B-->
            <Image Grid.Row="3" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/wb.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="3" Grid.Column="1" Text="W/B"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="4" Grid.Column="1" Text="Victories/Battles"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="3" Grid.Column="2" Text="{Binding Rating_BattleAvgPerformanceValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleAvgPerformanceValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="4" Grid.Column="2" Text="{Binding Rating_BattleAvgPerformanceValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="3" Grid.Column="3" Text="{Binding Rating_BattleAvgPerformancePlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleAvgPerformancePlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="4" Grid.Column="3" Text="{Binding Rating_BattleAvgPerformancePlaceDelta}"></TextBlock>
            <!--E/B-->
            <Image Grid.Row="5" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/eb.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="5" Grid.Column="1" Text="E/B"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="6" Grid.Column="1" Text="Average Experience per Battle"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="5" Grid.Column="2" Text="{Binding Rating_BattleAvgXpValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleAvgXpValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="6" Grid.Column="2" Text="{Binding Rating_BattleAvgXpValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="5" Grid.Column="3" Text="{Binding Rating_BattleAvgXpPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleAvgXpPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="6" Grid.Column="3" Text="{Binding Rating_BattleAvgXpPlaceDelta}"></TextBlock>
            <!--WIN-->
            <Image Grid.Row="7" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/win.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="7" Grid.Column="1" Text="WIN"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="8" Grid.Column="1" Text="Victories"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="7" Grid.Column="2" Text="{Binding Rating_BattleWinsValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleWinsValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="8" Grid.Column="2" Text="{Binding Rating_BattleWinsValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="7" Grid.Column="3" Text="{Binding Rating_BattleWinsPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattleWinsPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="8" Grid.Column="3" Text="{Binding Rating_BattleWinsPlaceDelta}"></TextBlock>
            <!--GPL-->
            <Image Grid.Row="9" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/gpl.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="9" Grid.Column="1" Text="GPL"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="10" Grid.Column="1" Text="Battles Participated"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="9" Grid.Column="2" Text="{Binding Rating_BattlesValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattlesValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="10" Grid.Column="2" Text="{Binding Rating_BattlesValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="9" Grid.Column="3" Text="{Binding Rating_BattlesPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_BattlesPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="10" Grid.Column="3" Text="{Binding Rating_BattlesPlaceDelta}"></TextBlock>
            <!--CPT-->
            <Image Grid.Row="11" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/cpt.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="11" Grid.Column="1" Text="CPT"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="12" Grid.Column="1" Text="Capture Points"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="11" Grid.Column="2" Text="{Binding Rating_CapturedPointsValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_CapturedPointsValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="12" Grid.Column="2" Text="{Binding Rating_CapturedPointsValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="11" Grid.Column="3" Text="{Binding Rating_CapturedPointsPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_CapturedPointsPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="12" Grid.Column="3" Text="{Binding Rating_CapturedPointsPlaceDelta}"></TextBlock>
            <!--DMG-->
            <Image Grid.Row="13" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/dmg.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="13" Grid.Column="1" Text="DMG"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="14" Grid.Column="1" Text="Damage Caused"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="13" Grid.Column="2" Text="{Binding Rating_DamageDealtValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_DamageDealtValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="14" Grid.Column="2" Text="{Binding Rating_DamageDealtValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="13" Grid.Column="3" Text="{Binding Rating_DamageDealtPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_DamageDealtPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="14" Grid.Column="3" Text="{Binding Rating_DamageDealtPlaceDelta}"></TextBlock>
            <!--DPT-->
            <Image Grid.Row="15" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/dpt.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="15" Grid.Column="1" Text="DPT"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="16" Grid.Column="1" Text="Defense Points"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="15" Grid.Column="2" Text="{Binding Rating_DroppedPointsValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_DroppedPointsValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="16" Grid.Column="2" Text="{Binding Rating_DroppedPointsValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="15" Grid.Column="3" Text="{Binding Rating_DroppedPointsPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_DroppedPointsPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="16" Grid.Column="3" Text="{Binding Rating_DroppedPointsPlaceDelta}"></TextBlock>
            <!--FRG-->
            <Image Grid.Row="17" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/frg.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="17" Grid.Column="1" Text="FRG"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="18" Grid.Column="1" Text="Targets Destroyed"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="17" Grid.Column="2" Text="{Binding Rating_FragsValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_FragsValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="18" Grid.Column="2" Text="{Binding Rating_FragsValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="17" Grid.Column="3" Text="{Binding Rating_FragsPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_FragsPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="18" Grid.Column="3" Text="{Binding Rating_FragsPlaceDelta}"></TextBlock>
            <!--SPT-->
            <Image Grid.Row="19" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/spt.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="19" Grid.Column="1" Text="SPT"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="20" Grid.Column="1" Text="Targets Detected"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="19" Grid.Column="2" Text="{Binding Rating_SpottedValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_SpottedValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="20" Grid.Column="2" Text="{Binding Rating_SpottedValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="19" Grid.Column="3" Text="{Binding Rating_SpottedPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_SpottedPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="20" Grid.Column="3" Text="{Binding Rating_SpottedPlaceDelta}"></TextBlock>
            <!--EXP-->
            <Image Grid.Row="21" Grid.RowSpan="2" Grid.Column="0" Source="..\Resources/Images/exp.png" Stretch="None"></Image>
            <TextBlock Foreground="#BABFBA" Grid.Row="21" Grid.Column="1" Text="EXP"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="22" Grid.Column="1" Text="Total Experience"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="21" Grid.Column="2" Text="{Binding Rating_XpValue}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_XpValueDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}}" Grid.Row="22" Grid.Column="2" Text="{Binding Rating_XpValueDelta}"></TextBlock>
            <TextBlock Foreground="#BABFBA" Grid.Row="21" Grid.Column="3" Text="{Binding Rating_XpPlace}"></TextBlock>
            <TextBlock Foreground="{Binding Rating_XpPlaceDelta, Converter={x:Static Converters:DeltaToColorConverter.Default}, ConverterParameter=True}" Grid.Row="22" Grid.Column="3" Text="{Binding Rating_XpPlaceDelta}"></TextBlock>
        </Grid>
        <Grid Grid.Row="4">
            <d3:ChartPlotter Name="Chart" LegendVisibility="Hidden" Height="300" Width="300" MainHorizontalAxisVisibility="Visible" MainVerticalAxisVisibility="Visible" Background="Transparent"
                              Margin="10,10,0,10">
            </d3:ChartPlotter>
        </Grid>
    </Grid>
</UserControl>
