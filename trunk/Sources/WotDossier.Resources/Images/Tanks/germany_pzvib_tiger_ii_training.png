 function SearchClan()
{
	$.getJSON('wot_search_clan.php?search=' + document.wotsearch_clan.searchclan.value, function(data)
	{
		$('.wotsearch_clan').empty();
	
		if (data.length == 0)
		{
			$item = '<li>No Clan found</li>';
	    $('.wotsearch_clan').append($item);
      return;
    }
    else
		{
		  $.each(data, function(clankey, clan)
		  {
				$item = '<li><a href="/clan/' + clan.servername + '/' + clan.clanDBID + '/' + clan.clanseo + '">' + '[' + clan.tag + ']' + ' ' + clan.name + '</a><br/></li>';
		    $('.wotsearch_clan').append($item);
		  });
		}
	})
};
  

 
 
function SearchProfile()
{
	$.getJSON('wot_search.php?search=' + document.wotsearch.searchprofile.value, function(data)
	{
		$('.wotsearch').empty();
	
		if (data.length == 0)
		{
			$item = '<li>No profile found</li>';
	    $('.wotsearch').append($item);
      return;
    }
    else
		{
		  $.each(data, function(profilekey, profile)
		  {
				$item = '<li><a href="/wot.php?do=charts&amp;wotid=' + profile.wotid + '">' + profile.username + '</a><br/></li>';
		    $('.wotsearch').append($item);
		  });
		}
	})
};
 
 
