                <ScrollViewer>
                    <tabs:CommonTabNew x:Name="CommonTab" HorizontalAlignment="Stretch" DataContext="{Binding PlayerStatistic}"></tabs:CommonTabNew>
                </ScrollViewer>
            </TabItem>
            <TabItem TabIndex="0" Header="Charts" Name="tabChart">
